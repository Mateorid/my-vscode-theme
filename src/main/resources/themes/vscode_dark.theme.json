{
  "name": "VSCode Dark",
  "dark": true,
  "author": "Dinesh Srinivasan",
  "editorScheme": "/themes/vscode_dark.xml",
  "ui": {
    "*": {
      "background": "#323232"
    },
    "Borders": {
      "color": "#464545",
      "ContrastBorderColor": "#464545"
    },
    "Editor": {
      "Toolbar.borderColor": "#464545",
      "background": "#323232"
    },
    "EditorTabs": {
      "background": "#252526",
      "tabInsets": true,
      "underlinedTabBackground": "#1e1e1e",
      "inactiveUnderlineColor": "#2d2d2d",
      "inactiveColoredFileBackground": "#323232",
      "underlineHeight": 2,
      "borderColor": "#464545"
    },
    "Panel": {
      "foreground": "#acacac",
      "background": "#333333"
    },
    "Tree": {
      "background": "#242425",
      "foreground": "#c7c7c7",
      "selectionForeground": "#f8fafb"
    },
    "Menu": {
      "background": "#323232",
      "foreground": "#cbcbc2",
      "borderColor": "#141414",
      "separatorColor": "#606060"
    },
    "PopupMenuSeparator": {
      "height": 13,
      "stripeIndent": 6,
      "stripeWidth": 1
    },
    "MenuBar": {
      "borderColor": "#323232"
    },
    "MenuItem": {
      "background": "#242425",
      "foreground": "#cbcbc2",
      "selectionBackground": "#094770"
    },
    "NavBar": {
      "borderColor": "#323232"
    },
    "CompletionPopup": {
      "background": "#242425",
      "foreground": "#cecece"
    },
    "Button": {
      "arc": 2
    },
    "StatusBar": {
      "borderColor": "#323232"
    },

    "ToolWindow": {
      "Header": {
        "borderColor": "#464545",
        "background": "#3b4754",
        "inactiveBackground": "#323232"
      }
    },
    "WelcomeScreen": {
      "background": "#333333",
      "SidePanel.background": "3c3f41",
      "Projects.background": "#333333",
      "Projects.actions.background": "#3c5c86"
    }
  }
}
